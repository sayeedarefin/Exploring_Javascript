//variable name cannot be any reserved keyword
var false = 66;
var return = true;

//2. variable name has to be in one word, no space
var my home address = "lalkhan bazar";

// 3. variable name cannot have qoutation

var "name"= "tom hanks";

// 4. variable name cannot starts with a number, but can ends with a number

var 99club = 564;
var club99 = 654;

// how to use long names
// we cant use dashes

var user-name = "raj bappa";

var user_name= "bappa raj"; //max use in python

var usercurrenthomeaddress = "lalkhan bazar"; //bujha jaccchena kicchu

var user_home_address= "lalkhan bajar"; //snake case

var userHomeAddress = "lalkhan bazar"; //camel case, we will use this in javascript

var UserHomeAddress = "lalkhan bajar"; // pascal case


// variable name is case sensitive

var person = 36;
var Person = 25;